{
  "name": "dxpool-keygen",
  "productName": "DxPool Key Gen",
  "version": "0.1.1",
  "description": "GUI key generation tool wrapping the deposit cli.",
  "main": "./build/electron/index.js",
  "author": "Stake DxPool <stake.dxpool.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@types/babel__core": "^7",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.11",
    "@types/tmp": "^0.2.2",
    "autoprefixer": "^10.4.19",
    "electron": "^28.2.2",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "webpack --config webpack.react.config.js --config webpack.electron.config.js",
    "build:watch": "webpack --config webpack.react.config.js --config webpack.electron.config.js --watch",
    "buildcli": "src/scripts/bundle_proxy_linux.sh",
    "buildcliwin": "src/scripts/bundle_proxy_win.bat",
    "start": "electron ./build/electron/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -m"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.10",
    "@mui/styled-engine-sc": "^6.0.0-alpha.16",
    "@mui/styles": "^5.15.10",
    "@mui/x-data-grid": "^7.4.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "axios": "^1.7.2",
    "babel-loader": "^9.1.3",
    "bls-eth-wasm": "^1.2.1",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "idb": "^8.0.0",
    "network-checker-tool": "^1.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-virtuoso": "^4.7.10",
    "shebang-loader": "^0.0.1",
    "ssv-keys": "^1.1.0",
    "ssv-scanner": "git+https://github.com/bloxapp/ssv-scanner.git",
    "style-loader": "^3.0.0",
    "styled-components": "^6.1.8",
    "typeface-roboto": "^1.1.13",
    "web3-utils": "^1.7.0"
  },
  "build": {
    "appId": "com.dxpool.keygen",
    "productName": "DxPool Key Gen",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "extraFiles": [
      "build/bin/*",
      "build/word_lists/*",
      "static/icon.png"
    ],
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ],
      "category": "public.app-category.utilities",
      "icon": "static/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "static/icon.png"
    },
    "win": {
      "target": "portable",
      "icon": "static/icon.ico"
    }
  },
  "packageManager": "yarn@4.1.0"
}
